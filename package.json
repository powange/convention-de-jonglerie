{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "geocode": "node scripts/run-geocoding.mjs",
    "db:clean-tokens": "npx tsx scripts/clean-expired-tokens.ts",
    "admin:add": "npx tsx scripts/manage-admin.ts add",
    "admin:remove": "npx tsx scripts/manage-admin.ts remove",
    "admin:list": "npx tsx scripts/manage-admin.ts list",
    "help": "node scripts/show-help.mjs",
    "test": "vitest --config vitest.config.simple.ts",
    "test:ui": "vitest --ui --config vitest.config.simple.ts",
    "test:run": "vitest run --config vitest.config.simple.ts",
    "test:nuxt": "vitest",
    "test:nuxt:run": "vitest run",
    "test:db": "cross-env TEST_WITH_DB=true vitest",
    "test:db:run": "node scripts/test-db-run.js",
    "test:setup": "docker-compose -f docker-compose.test.yml up -d --wait && node scripts/migrate-test.js",
    "test:teardown": "docker-compose -f docker-compose.test.yml down"
  },
  "dependencies": {
    "@internationalized/date": "^3.8.2",
    "@nuxt/eslint": "^1.7.1",
    "@nuxt/image": "^1.10.0",
    "@nuxt/scripts": "^0.11.10",
    "@nuxt/test-utils": "^3.19.2",
    "@nuxt/ui": "^3.3.0",
    "@nuxtjs/i18n": "^10.0.3",
    "@pinia/nuxt": "^0.11.2",
    "@prisma/client": "^6.13.0",
    "@types/nodemailer": "^6.4.17",
    "@unhead/vue": "^2.0.12",
    "@vueuse/core": "^13.6.0",
    "@vueuse/nuxt": "^13.6.0",
    "bcryptjs": "^3.0.2",
    "eslint": "^9.32.0",
    "flag-icons": "^7.5.0",
    "jsonwebtoken": "^9.0.2",
    "md5": "^2.3.0",
    "nodemailer": "^7.0.5",
    "nuxt": "^4.0.1",
    "pinia": "^3.0.3",
    "typescript": "^5.8.3",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@iconify-json/heroicons": "^1.2.2",
    "@testing-library/vue": "^8.1.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "cross-env": "^10.0.0",
    "happy-dom": "^18.0.1",
    "prisma": "^6.13.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "wait-on": "^8.0.4"
  }
}
